<div class="artists">

    <div class="separator container"><h3>{{__ 'composers.inResidence'}}</h3></div>
    <div class="card-columns">
       
        {{#each composers.InResidence}}
        {{> composerCard composer=this}}
        {{/each}}     
    </div>
    <br>

    <div class="separator container"><h3>{{__ 'composers.Partners'}}</h3></div>
    <div class="card-columns">        
        {{#each composers.Partners}}
        {{> composerCard composer=this}}
        {{/each}}     
    </div>

</div>