<div class="artists">
    <h4 class="text-center mb-3">{{__ 'artists.title'}}</h4>



    <h5 class="text-center mb-3">{{__ 'artists.conductors'}}</h5>
    <div class="card-columns">
        {{#each artists.[0]}}
        <div class="card "> <img class="card-img-top"  loading=lazy src="/img/about/artists/{{this.id}}.jpg" alt="Card image cap">
            <div class="card-footer"><p>{{this.name}} ({{this.instrument}}, {{this.country}})</p></div>
        </div>
        {{/each}}     
